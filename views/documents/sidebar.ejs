<div class="nav-left">

    <h2>Documentation</h2>

    <ul>

        <li><a href="/docs" class="name"><span class="icon arrow_carrot-right" ></span>Show All</a></li>

        <% subjects.sort().map(function(subject){ %>
        <li><a href="/docs/<%- subject.id %>"><span class="icon arrow_carrot-right"></span><span
                        class="name"><%- subject.name %></span></a></li>
        <% }) %>
    </ul>

    <% if(locals.signedInUser && locals.signedInUser.isAdmin){ %>
    <div class="tile pad">
        <h4>Admin</h4>
        <div class="form-group">
            <li><a href="/docs/new" class="button primary">New</a></li>
        </div>
    </div>
    <% } %>
</div>